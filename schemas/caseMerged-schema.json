asyncapi: 2.3.0
info:
  title: Court Case Merged Event
  version: 0.0.1
  description: This schema defines the court case merged event
channels:
  courtCases:
    subscribe:
      message:
        $ref: '#/components/messages/CaseMerged'
components:
  messages:
    CaseApproved:
      payload:
        type: object
        properties:
          number:
            type: string
            description: Court file number for the merged case.  Appears on all court documents related to the case. Reference number for the case and guides how documents are gathered and reviewed.
            example: 135
          previousNumber:
            type: string
            description: Court file number for the case prior to the merge.  Appears on all court documents related to the case. Reference number for the case and guides how documents are gathered and reviewed.
            example: 246            
