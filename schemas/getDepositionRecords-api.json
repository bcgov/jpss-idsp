---
openapi: 3.0.2
info:
  title: Get Case Deposition API
  version: 0.0.1
  description: This operation allows the calling application to retrieve the deposition records associated with the court case.
paths:
  /bcpsUsers/{userId}/courtCases/{caseNumber}/getDepositionRecords:
    get:
      parameters:
      - name: userId
        in: path
        description: user id
        required: true
        schema:
          type: string
      - name: caseNumber
        in: path
        description: case number
        required: true
        schema:
          type: string
      responses:
        200:
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    depositionId:
                      type: string
                    base64data:
                      type: string
          description: Deposition Records
        401:
          description: Not authorized
          content: {}
        404:
          description: user id or case number not found
          content: {}
      operationId: getDepositionRecords